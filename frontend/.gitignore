# ===========================
# Angular standard .gitignore
# ===========================

# compiled output
/dist
/tmp
/out-tsc
/build

# dependencies
/node_modules

# IDEs and editors
/.idea
/.vscode
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?
*.log

# environment files
/src/environments/environment*.ts

# misc
/.angular/cache
/.angular/config.json
/.angular/analytics.json
/.angular/local
/.angular/stats.json

# system files
.DS_Store
Thumbs.db

# optional: OS files
ehthumbs.db
Desktop.ini

# coverage reports
/coverage

# firebase / cloud
/.firebase
/firebase-debug.log
/firebase-debug.*.log

# testing
/karma-*.log
/testem.log
/typings

# build tools / package managers
package-lock.json
yarn-error.log

# optional: local tooling
/.npm
/.pnpm-store
/.yarn
/.env
/.env.local
/.env.*.local

# optional: docker / dev stuff
docker-compose.override.yml